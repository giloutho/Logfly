{
  "name": "logfly",
  "productName": "Logfly",
  "version": "0.0.2",
  "description": "Paragliding flights logbook",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "giloutho",
    "email": "gilouthomas@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/logo/logo6",
        "name": "Logfly"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "logfly"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ],
          "config": {
            "background": "src/assets/logo/dmg-background.png",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "src/assets/logo/logo6.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "array-smooth": "^1.0.0",
    "better-sqlite3": "^7.5.0",
    "bootstrap": "^4.6.1",
    "bootstrap-select": "^1.13.18",
    "datatables.net-bs4": "^1.11.5",
    "datatables.net-select-bs4": "^1.3.4",
    "drivelist": "^9.2.4",
    "electron-dl": "^3.3.1",
    "electron-fetch": "^1.9.1",
    "electron-log": "^4.4.6",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "form-data": "^4.0.0",
    "fs-jetpack": "^4.3.1",
    "geo-tz": "^7.0.1",
    "gettext.js": "^1.1.1",
    "glob": "^7.2.0",
    "highcharts": "^10.0.0",
    "iana-tz-data": "^2019.1.0",
    "igc-parser": "^1.1.0",
    "inputmask": "^5.0.7",
    "is-internet-available": "^3.1.0",
    "jquery": "^3.6.0",
    "jquery-contextmenu": "^2.9.2",
    "leaflet": "^1.7.1",
    "leaflet.control.layers.tree": "^1.1.0",
    "moment": "^2.29.4",
    "mustache": "^4.2.0",
    "popper.js": "^1.16.1",
    "properties-reader": "^2.2.0",
    "reverse-line-reader": "^0.2.6",
    "save": "^2.9.0",
    "serialport": "^10.4.0",
    "sharp": "^0.30.7",
    "simplify-geojson": "^1.0.4",
    "spherical-geometry-js": "2.0.0",
    "srtm-elevation": "^2.1.0",
    "zoned-date-time": "^1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "17.1.2",
    "electron-rebuild": "^3.2.7"
  }
}
